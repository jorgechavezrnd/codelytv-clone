{
  "name": "jorge/codelytv-clone",
  "license": "MIT",
  "type": "project",
  "description": "Clone of codelytv pro platform applying Domain-Driven Design, Hexagonal Architecture and CQRS in a Monorepository",
  "authors": [
    {
      "name": "jorgechavezrnd",
      "email": "jorgechavezrnd@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4",

    "ext-apcu": "*",
    "ext-json": "*",
    "ext-zend-opcache": "*",
    "ext-pdo": "*",

    "symfony/framework-bundle": "^5.1",
    "symfony/dotenv": "^5.1",
    "symfony/yaml": "^5.1"
  },
  "require-dev": {
    "ext-xdebug": "*",

    "roave/security-advisories": "dev-latest",

    "behat/behat": "^3.7",
    "friends-of-behat/mink-extension": "^2.4",
    "behat/mink-browserkit-driver": "^1.3",
    "friends-of-behat/symfony-extension": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "CodelyTv\\Apps\\Mooc\\Backend\\": "apps/mooc/backend/src",

      "CodelyTv\\": ["src"]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CodelyTv\\Tests\\": ["tests/src"]
    }
  },
  "minimum-stability": "stable"
}
